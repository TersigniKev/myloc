{{ form_start(form) }}
  <div class="container-fluid w-75">
    <div class="mb-3 row align-items-center">
      <label class="col-sm-3 col-form-label">{{ form_label(form.start) }}</label>
      <div class="col-sm-9">
        {{ form_widget(form.start, {'attr': {'class': 'form-control'}}) }}
        {{ form_errors(form.start) }}
      </div>
    </div>
    <div class="mb-3 row align-items-center">
      <label class="col-sm-3 col-form-label">{{ form_label(form.endLoan) }}</label>
      <div class="col-sm-9">
        {{ form_widget(form.endLoan, {'attr': {'class': 'form-control'}}) }}
        {{ form_errors(form.endLoan) }}
      </div>
    </div>
  </div>
  <div class="mt-3">
    <button type="submit" class="btn btn-outline-primary">{{ button_label|default('Enregistrer') }}</button>
    {# <a href="{{ path('app_membres') }}" class="btn btn-outline-light ms-2">Retour</a> #}
  </div>
{{ form_end(form) }}