{{ form_start(form) }}
  <div class="container-fluid w-75">
    <div class="mb-3 row align-items-center">
      <label class="col-sm-3 col-form-label">{{ form_label(form.name) }}</label>
      <div class="col-sm-9">
        {{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}
        {{ form_errors(form.name) }}
      </div>
    </div>
    <div class="mb-3 row align-items-center">
      <label class="col-sm-3 col-form-label">{{ form_label(form.description) }}</label>
      <div class="col-sm-9">
        {{ form_widget(form.description, {'attr': {'class': 'form-control'}}) }}
        {{ form_errors(form.description) }}
      </div>
    </div>
    <div class="mb-3 row align-items-center">
      <label class="col-sm-3 col-form-label">{{ form_label(form.picture) }}</label>
      <div class="col-sm-9">
        {{ form_widget(form.picture, {'attr': {'class': 'form-control'}}) }}
        {{ form_errors(form.picture) }}
      </div>
    </div>
    <div class="mb-3 row align-items-center">
      <label class="col-sm-3 col-form-label">{{ form_label(form.category) }}</label>
      <div class="col-sm-9">
        {{ form_widget(form.category, {'attr': {'class': 'form-control'}}) }}
        {{ form_errors(form.category) }}
      </div>
    </div>
  </div>
  <div class="mt-3">
    <button type="submit" class="btn btn-outline-primary">{{ button_label|default('Enregistrer') }}</button>
    {# <a href="{{ path('app_membres') }}" class="btn btn-outline-light ms-2">Retour</a> #}
  </div>
{{ form_end(form) }}